package inheritance;

public class This_keyword {
	  
		int rollno;  
		String name;  
		float fee;  
		This_keyword(int rollno,String name,float fee){  
		this.rollno=rollno;  
		this.name=name;  
		this.fee=fee;  
		}  
		void display(){System.out.println(name+rollno+" "+" "+fee);}  
		}  
		  
		
