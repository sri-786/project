package inheritance;

public class This_keyword {
	
	  
		int rollno;  
		String name;  
		float fee;  
		This_keyword(int rollno,String name,float fee){  
		rollno=rollno;  
		name=name;  
		fee=fee;  
		}  
		void display(){System.out.println(rollno+" "+name+" "+fee);}  
		}  
